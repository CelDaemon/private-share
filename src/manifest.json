{
    "manifest_version": 3,
    "name": "Private Share",
    "version": "dev",
    "description": "Remove tracking from share functions",
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_start",
            "world": "MAIN"
        }
    ],
    "browser_specific_settings": {
        "gecko": {
            "id": "private-share@voidgroup.net"
        }
    }
}
